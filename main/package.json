{
  "name": "main",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn configure-release && yarn build-release",
    "build-debug": "cmake --build ../build --config Debug",
    "build-release": "cmake --build ../build --config Release",
    "configure-debug": "cmake -S . -B ../build -D CMAKE_BUILD_TYPE=Debug",
    "configure-release": "cmake -S . -B ../build -D CMAKE_BUILD_TYPE=Release",
    "start": "yarn configure-debug && yarn build-debug && run-script-os",
    "start:macos": "'../build/AudioPluginExample_artefacts/Debug/Standalone/Audio Plugin Example.app/Contents/MacOS/Audio Plugin Example'",
    "start:windows": "'../build/AudioPluginExample_artefacts/Debug/Standalone/Audio Plugin Example.exe'"
  },
  "devDependencies": {
    "run-script-os": "^1.1.6"
  }
}
